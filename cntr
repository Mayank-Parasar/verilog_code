#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1703730 .scope module, "counter_tb" "counter_tb" 2 4;
 .timescale 0 0;
v0x174a980_0 .var "clock", 0 0;
v0x174aa50_0 .net "counter_out", 3 0, v0x174a790_0; 1 drivers
v0x174ab00_0 .var "enable", 0 0;
v0x174abb0_0 .var "reset", 0 0;
S_0x1703820 .scope module, "U_counter" "counter" 2 32, 3 9, S_0x1703730;
 .timescale 0 0;
v0x17396c0_0 .net "clk", 0 0, v0x174a980_0; 1 drivers
v0x174a790_0 .var "counter_out", 3 0;
v0x174a830_0 .net "enable", 0 0, v0x174ab00_0; 1 drivers
v0x174a8d0_0 .net "rst", 0 0, v0x174abb0_0; 1 drivers
E_0x1715b00 .event posedge, v0x174a8d0_0, v0x17396c0_0;
S_0x17395d0 .scope begin, "COUNTER" "COUNTER" 3 35, 3 35, S_0x1703820;
 .timescale 0 0;
    .scope S_0x1703820;
T_0 ;
    %wait E_0x1715b00;
    %fork t_1, S_0x17395d0;
    %jmp t_0;
    .scope S_0x17395d0;
t_1 ;
    %load/v 8, v0x174a8d0_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 4, 0;
    %assign/v0 v0x174a790_0, 1, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x174a830_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v0x174a790_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v0x174a790_0, 1, 8;
T_0.2 ;
T_0.1 ;
    %end;
    .scope S_0x1703820;
t_0 %join;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1703730;
T_1 ;
    %vpi_call 2 11 "$dumpfile", "counter.vcd";
    %vpi_call 2 12 "$dumpvars", 1'sb0, S_0x1703730;
    %vpi_call 2 13 "$display", "time\011 clk reset enable counter";
    %vpi_call 2 14 "$monitor", "%g\011 %b  %b  %b", $time, v0x174a980_0, v0x174abb0_0, v0x174ab00_0, v0x174aa50_0;
    %set/v v0x174a980_0, 1, 1;
    %set/v v0x174abb0_0, 0, 1;
    %set/v v0x174ab00_0, 0, 1;
    %delay 5, 0;
    %set/v v0x174abb0_0, 1, 1;
    %delay 10, 0;
    %set/v v0x174abb0_0, 0, 1;
    %delay 10, 0;
    %set/v v0x174ab00_0, 1, 1;
    %delay 100, 0;
    %set/v v0x174ab00_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 23 "$finish";
    %end;
    .thread T_1;
    .scope S_0x1703730;
T_2 ;
    %delay 5, 0;
    %load/v 8, v0x174a980_0, 1;
    %inv 8, 1;
    %set/v v0x174a980_0, 8, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_tb.v";
    "./counter.v";
